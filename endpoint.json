{
    "label": "QuickChart",
    "name": "quickchart",
    "apiVersion": "v1",
    "internal": false,
    "configurationType": "GLOBAL",
    "deployment": {
        "profiles":[
            {
                "name": "default",
                "label": "Default",
                "memory": 256,
                "description": "Default configuration"
            }
        ],
        "allowMultipleInstances": true
    },
    "icon48": "https://storage.googleapis.com/static.slingrs.io/platform/endpoints/quickchart.png",
    "status": "ACTIVE",
    "stores": [],
    "events": [
        {
            "label": "Chart Response",
            "name": "chartResponse"
        },
        {
            "label": "QR Response",
            "name": "qrResponse"
        }
    ],
    "functions": [
        {
            "label": "POST",
            "name": "_post",
            "description": "Sends a POST request",
            "callbacks": [
                {
                    "name": "chartResponse",
                    "maxWaitingTime": 120000,
                    "maxExpectedResponses": 1
                }
            ]
        },
        {
            "label": "GET",
            "name": "_get",
            "description": "Sends a GET request",
            "callbacks": [
                {
                    "name": "qrResponse",
                    "maxWaitingTime": 120000,
                    "maxExpectedResponses": 1
                }
            ]
        }
    ],
    "scripts":[
        "functions.js"
    ],
    "flowSteps": [
        "apiCallQuickchart"
    ],
    "configuration":[
        {
            "name": "key",
            "label": "Key",
            "type": "text",
            "description": "Required for QuickChart Pro.",
            "required": false
        }
    ]
}